{
  "name": "stylelint-config-strict",
  "version": "1.0.0",
  "description": "strict shareable config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "strict"
  ],
  "author": "maqiang1816@foxmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maqiang1816/stylelint-config-strict.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "eslint": "^6.0.0",
    "eslint-config-stylelint": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "npmpub": "^4.1.0",
    "stylelint": "^10.1.0"
  },
  "peerDependencies": {
    "stylelint": "^8.3.0 || ^9.0.0 || ^10.0.0"
  },
  "scripts": {
    "dry-release": "npmpub --dry --verbose",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "release": "npmpub"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  }
}
